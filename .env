## PHP source directory#SOURCE_DIR=./www## Runtime data directory#DATA_DIR=./data## Container Timezone#TZ=Asia/Shanghai## UBUNTU#UBUNTU_IMAGES=ubuntu:18.04## LEAF ZOOKEEPER#ZOOKEEPER_IMAGES=zookeeper:3.6LEAF_ZOOKEEPER_LOG_DIR=./logs/leaf-zookeeper## LEAF SERVER#LEAF_SERVER_IMAGES=openjdk:8u265-jre## NODE#NODE_IMAGES=node:10.16.0-alpine## GO#GO_IMAGES=golang:latest## Nginx# 更多版本选择：https://hub.docker.com/r/openresty/openresty/tags#NGINX_IMAGES=openresty/openresty:1.19.9.1-2-bionicNGINX_HTTP_HOST_PORT=80NGINX_HTTPS_HOST_PORT=443NGINX_CONFD_DIR=./services/nginx/conf.dNGINX_CONF_FILE=./services/nginx/nginx.confNGINX_LOG_DIR=./logs/nginxLUA_DIR=./services/lua## COMPOSER#COMPOSER_DATA_DIR=./data/composerCOMPOSER_VERSION=1.8.6COMPOSER_PACKAGIST=https://mirrors.aliyun.com/composer/## PHP74#PHP74_IMAGES=php:7.4.27-fpm-alpineALPINE_REPOSITORIES=mirrors.tuna.tsinghua.edu.cnPHP74_PHP_CONF_FILE=./services/php74/php.iniPHP74_FPM_CONF_FILE=./services/php74/php-fpm.confPHP74_LOG_DIR=.logs/phpPHP74_EXTENSIONS=,pdo_mysql,mysqli,gd,opcache,redis,zip,pcntl,exif,bcmath,sockets,gettext,intl,xmlrpc,mcrypt,mongodb,## MySQL57#MYSQL57_IMAGES=mysql:5.7.28MYSQL57_HOST_PORT=3306MYSQL57_ROOT_PASSWORD=123456MYSQL57_CONF_FILE=./services/mysql57/mysql.cnf## Redis#REDIS_IMAGES=redis:5.0.9-alpineREDIS_HOST_PORT=6379REDIS_CONF_FILE=./services/redis/redis.confREDIS_PASSWORD=123456## MONGODB#MONGODB_IMAGES=mongo:4.1MONGODB_HOST_PORT=27017MONGODB_INITDB_ROOT_USERNAME=rootMONGODB_INITDB_ROOT_PASSWORD=123456## RABBITMQ#RABBITMQ_VERSION=managementRABBITMQ_CONF_FILE=./services/rabbitmq/rabbitmq.ymlRABBITMQ_HOST_PORT_C=5672RABBITMQ_HOST_PORT_S=15672RABBITMQ_DEFAULT_USER=rootRABBITMQ_DEFAULT_PASS=123456RABBITMQ_PLUGINS=rabbitmq_amqp1_0## ELASTICSEARCH#ELASTICSEARCH_VERSION=7.1.1ELASTICSEARCH_CONF_FILE=./services/elasticsearch/elasticsearch.ymlELASTICSEARCH_HOST_PORT_C=9200ELASTICSEARCH_HOST_PORT_S=9300ELASTICSEARCH_PLUGINS=analysis-ik## KIBANA#KIBANA_VERSION=7.1.1KIBANA_HOST=5601KIBANA_I18N_LOCALE=zh-CN